server {
	listen 17
	location /http_no_port {
		redirect_to naver.com
	}
	location /http_port {
		redirect_to naver.com:8080
	}
	location /http_protoc_no_port {
		redirect_to http://naver.com
	}
	location /https_protoc_no_port {
		redirect_to https://naver.com
	}
	location /https_protoc_port {
		redirect_to https://naver.com:80
	}
	location /https_only_port {
		redirect_to naver.com:443
	}
}
